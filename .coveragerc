[run]
source = ch_filing
branch = true
omit = 
    */tests/*
    */test_server/*
    */__main__.py
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
show_contexts = true

[xml]
output = coverage.xml